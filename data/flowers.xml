<flowers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://www.yuditsky.com/flowers"
         xsi:schemaLocation="http://www.yuditsky.com/flowers ../data/flowers.xsd">

    <flower id="ID-1" species="Currant">
        <soil>podzolic</soil>
        <origin>Eurasia</origin>
        <growing_tipc>
            <temperature>20</temperature>
            <photophilous>true</photophilous>
            <watering>500</watering>
        </growing_tipc>
        <visual_param>
            <stem_color>green</stem_color>
            <leaf_color>green</leaf_color>
            <average_size>150</average_size>
        </visual_param>
        <multiplying>cutting</multiplying>
    </flower>

    <flower id="ID-2" species="Tulip">
